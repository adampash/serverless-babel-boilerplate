{
  "name": "serverless-starter",
  "version": "1.0.0",
  "main": "src/handler.js",
  "license": "MIT",
  "scripts": {
    "deploy:env": "sls deploy --stage $NODE_ENV && yarn deploy:static",
    "deploy": "export NODE_ENV=dev && yarn deploy:env",
    "deploy:prod": "export NODE_ENV=prod && yarn deploy:env",
    "deploy:stage": "export NODE_ENV=stage && yarn deploy:env",
    "deploy:static": "yarn build:static && serverless client deploy",
    "watch:hello": "serverless webpack watch --function hello --path fixtures/event.json",
    "tail:hello": "serverless logs --function hello --tail",
    "test": "jest --watch",
    "serve": "serverless webpack serve",
    "build:static": "export NODE_ENV=prod && webpack --progress --colors --config web.webpack.config.js",
    "webpack-dev-server": "webpack-dev-server --inline --hot --progress --colors --watch --config web.webpack.config.js",
    "start": "export NODE_ENV=dev && concurrently -k \"yarn serve\" \"yarn webpack-dev-server\""
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^18.1.0",
    "serverless": "^1.5.1",
    "serverless-client-s3": "serverless/serverless-client-s3#1e074d1",
    "serverless-webpack": "^1.0.0-rc.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
